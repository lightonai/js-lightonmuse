{
	"name": "lighton-muse",
	"version": "1.0.2",
	"private": false,
	"license": "MIT",
	"type": "module",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "tsup src/index.ts --format esm --dts --target es2020",
		"build-examples": "tsup examples/*.ts --format esm -d examples --target esnext",
		"dev": "tsup src/index.ts --format esm --watch --dts --target es2020",
		"lint": "eslint src/*.ts --fix && prettier . --write",
		"docs": "typedoc src/index.ts",
		"tsc": "tsc",
		"prepublish": "yarn build"
	},
	"dependencies": {
		"node-fetch": "^3.2.6"
	},
	"devDependencies": {
		"@swc/core": "^1.2.205",
		"@types/node": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^5.8.0",
		"@typescript-eslint/parser": "^5.8.0",
		"eslint": "^8.18.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-sort-destructure-keys": "^1.4.0",
		"tsup": "^6.1.2",
		"typedoc": "^0.23.1",
		"typescript": "^4.7.4"
	},
	"files": [
		"assets/",
		"dist/",
		"LICENCE",
		"README.md"
	],
	"engines": {
		"node": ">=14.13.0"
	}
}
